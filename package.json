{
  "name": "helix-eye",
  "version": "'",
  "description": "Helix Eye is an app that consumes the NASA's API to display their records",
  "main": "index.js",
  "author": "edevars <e.devars.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-server": "webpack --config webpack.config.server.js",
    "build-client": "webpack --config webpack.config.client.js",
    "build": "NODE_ENV=production npm run build-server && npm run build-client",
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node dist/index.js",
    "build-dev": "NODE_ENV=development npm run build-server && NODE_ENV=development npm run build-client && node dist/index.js",
    "dev": "nodemon --ext .ts --exec \"npm run start\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.9.0",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "@types/webpack-node-externals": "^3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^3.1.10",
    "swc-loader": "^0.2.3",
    "typescript": "^5.8.3",
    "webpack": "5.99.9",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "5.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
